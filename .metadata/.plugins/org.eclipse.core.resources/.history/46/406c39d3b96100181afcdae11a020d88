package com.jani.selenium;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.ie.InternetExplorerDriver;


public class SeleniumWebDriver {

	public static void Chromewebdriver() {
    	System.setProperty("webdriver.chrome.driver","D:\\Automation\\Selenium\\Resources\\Chromedriver.exe");
			
    }

	public static void firefoxdriver() {
		System.setProperty("webdriver.firefox.marionette","D:\\Automation\\Selenium\\Resources\\geckodriver.exe");
		WebDriver driver = new FirefoxDriver();
	}
	
	public static void IEdriver() {
		System.setProperty("webdriver.ie.driver","D:\\Automation\\Selenium\\Resources\\IEDriver.exe");
		WebDriver driver = new InternetExplorerDriver();
	}
}
